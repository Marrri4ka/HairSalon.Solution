<h1>Hair Salon</h1>

<h2>Specialities:</h2>

@if (Model.Count != 0)
{
  <ul>
    @foreach (var speciality in Model)
    {
      <li><a href='/specialities/@speciality.GetId()'>@speciality.GetName()</a></li>
    }
  </ul>
}

<h4><a href="/specialities/new">Add new speciality</a></h4>
<h4><a href="/">Return home</a></h4>
