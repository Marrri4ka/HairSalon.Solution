<h4>Stylists</h4>
@if(@Model.Count == 0)
{
<p>No stylists</p>
}

@foreach (var stylist in Model)
{
<li>  <a href='/stylists/@stylist.GetId()'>@stylist.GetName()</a>

  <a href='/stylists/@stylist.GetId()/delete'>Delete</a>
  <a href='/stylists/@stylist.GetId()/edit'>Edit</a>

  </li>
}
  <a class="card-link" href="/">Back home</a>
